quantity = int(input())
days_left = int(input())

spirit_points = 0
budget = 0

ornament_set = 2
tree_skirt = 5
tree_garland = 3
tree_lights = 15

for day in range ( 1,days_left + 1):
    if day % 11 == 0:
        quantity += 2
    if day % 2 == 0:
        budget += ornament_set * quantity
        spirit_points += 5
    if day % 3 == 0:
        budget += (tree_skirt + tree_garland) * quantity
        spirit_points += 3 + 10
    if day % 5 == 0:
        budget += tree_lights * quantity
        spirit_points += 17
        if day % 3 == 0:
            spirit_points += 30
    if day % 10 == 0:
        spirit_points -= 20
        budget += tree_lights + tree_skirt + tree_garland
if days_left % 10 == 0:
    spirit_points -= 30
print(f"Total cost: {budget}")
print(f"Total spirit: {spirit_points}")
